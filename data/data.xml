<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
         <!-- Sequences for hospital.patient -->
        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <data>
         <!-- Sequences for hospital.patient -->
        <record id="seq_hospital_ticket" model="ir.sequence">
            <field name="name">Ticket Sequence</field>
            <field name="code">hospital.ticket</field>
            <field name="prefix">OP</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <data>
         <!-- Sequences for hospital.patient -->
        <record id="seq_hospital_consultation" model="ir.sequence">
            <field name="name">Consultation Sequence</field>
            <field name="code">hospital.consultation</field>
            <field name="prefix">C</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>